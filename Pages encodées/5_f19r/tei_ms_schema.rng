      <ref name="tei_att.gaijiProp.attribute.name"/>
      <ref name="tei_att.gaijiProp.attribute.value"/>
      <ref name="tei_att.gaijiProp.attribute.version"/>
      <ref name="tei_att.gaijiProp.attribute.scheme"/>
